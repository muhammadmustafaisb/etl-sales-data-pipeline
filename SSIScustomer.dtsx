    <DTS:Variable
      DTS:CreationName=""
      DTS:DTSID="{80C9CAEA-942F-4209-BE08-7026593D6FAD}"
      DTS:EvaluateAsExpression="True"
      DTS:Expression="&quot;DECLARE @JSON VARCHAR(MAX); SELECT @JSON = BulkColumn FROM OPENROWSET (BULK '&quot; + @[User::FilePath] + &quot;', SINGLE_CLOB) AS import; SELECT CASE WHEN customer_id IS NULL OR LTRIM(RTRIM(customer_id)) = '' THEN CONCAT('UNKNOWN_', ABS(CHECKSUM(NEWID()))) ELSE customer_id END AS customer_id, CASE WHEN customer_name IS NULL OR LTRIM(RTRIM(customer_name)) = '' THEN 'Unknown' ELSE REPLACE(REPLACE(REPLACE(REPLACE(REPLACE(REPLACE(customer_name, CHAR(9), ''), CHAR(10), ''), CHAR(13), ''), '\&quot;', ''), '''', ''), ',','') END AS customer_name, email, region, CASE WHEN ISDATE(join_date) = 1 THEN CONVERT(DATE, join_date) ELSE NULL END AS join_date, loyalty_points FROM OPENJSON(@JSON) WITH (customer_id VARCHAR(50), customer_name VARCHAR(1000), email VARCHAR(500), region VARCHAR(50), join_date NVARCHAR(50), loyalty_points INT);&quot;"
      DTS:IncludeInDebugDump="2345"
      DTS:Namespace="User"
      DTS:ObjectName="Query">
      <DTS:VariableValue
        DTS:DataType="8">DECLARE @JSON VARCHAR(MAX); SELECT @JSON = BulkColumn FROM OPENROWSET (BULK 'C:\Users\muham\OneDrive\Desktop\Customer_data\customer_data.json', SINGLE_CLOB) AS import; SELECT CASE WHEN customer_id IS NULL OR LTRIM(RTRIM(customer_id)) = '' THEN CONCAT('UNKNOWN_', ABS(CHECKSUM(NEWID()))) ELSE customer_id END AS customer_id, CASE WHEN customer_name IS NULL OR LTRIM(RTRIM(customer_name)) = '' THEN 'Unknown' ELSE REPLACE(REPLACE(REPLACE(REPLACE(REPLACE(REPLACE(customer_name, CHAR(9), ''), CHAR(10), ''), CHAR(13), ''), '"', ''), '''', ''), ',','') END AS customer_name, email, region, CASE WHEN ISDATE(join_date) = 1 THEN CONVERT(DATE, join_date) ELSE NULL END AS join_date, loyalty_points FROM OPENJSON(@JSON) WITH (customer_id VARCHAR(50), customer_name VARCHAR(1000), email VARCHAR(500), region VARCHAR(50), join_date NVARCHAR(50), loyalty_points INT);</DTS:VariableValue>
    </DTS:Variable>
